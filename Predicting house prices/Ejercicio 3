# From previous steps
explanatory_data <- tibble(
  n_convenience = 0:10
)

# Edit this, so predictions are stored in prediction_data
prediction_data <- explanatory_data %>%
mutate(
    price_twd_msq = predict(
       mdl_price_vs_conv, explanatory_data
      )
  )

